<apex:page standardController="Contact" recordSetVar="contact" lightningStylesheets="true">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection columns="1">
                <apex:dataTable var="con" value="{!contact}">
                    <apex:column >
                        <apex:facet name="header">Name</apex:facet>
                        <apex:outputText value="{!con.Name}"></apex:outputText>
                    </apex:column>
                    <apex:column >
                        <apex:facet name="header">Account</apex:facet>
                        <apex:outputText value="{!con.Account}"></apex:outputText>
                    </apex:column>
                    <apex:column >
                        <apex:facet name="header">Name</apex:facet>
                        <apex:outputText value="{!con.Account}"></apex:outputText>
                    </apex:column>
                </apex:dataTable>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>